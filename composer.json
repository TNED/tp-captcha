{
    "name": "tned/tp-captcha",
    "description": "captcha package for thinkphp6",
    "authors": [
        {
            "name": "artlu",
            "email": "preferhu@qq.com"
        }
    ],
    "license": "MIT",
    "require": {
        "topthink/framework": "^6.0.0"
    },
    "autoload": {
        "psr-4": {
            "TNED\\tp-captcha\\": "src/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "extra": {
        "think": {
            "services": [
                "TNED\\captcha\\CaptchaService"
            ],
            "config":{
                "captcha": "src/config.php"
            }
        }
    }
}
